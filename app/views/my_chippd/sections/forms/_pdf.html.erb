<% if @page.page_template.name == 'Story' %>
  <%= form.input :kind, :collection => configatron.select_data.iconable_kinds.collect { |d| [d.to_s.titleize, d] }, :include_blank => false, :label => "Content Type" %>
<% else %>
  <%= form.input :kind, as: :hidden, input_html: { value: :other } %>
<% end %>
<%= render '/my_chippd/sections/forms/generic_pdf', :form => form %>
